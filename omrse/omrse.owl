    <!-- http://purl.obolibrary.org/obo/OBI_0000846 -->

    <owl:ObjectProperty rdf:about="&obo;OBI_0000846">
        <rdfs:label rdf:datatype="&xsd;string">is member of organization</rdfs:label>
        <IAO_0000116 rdf:datatype="&xsd;string">2009/10/01 Alan Ruttenberg. Barry prefers generic is-member-of. Question of what the range should be. For now organization. Is organization a population? Would the same relation be used to record members of a population</IAO_0000116>
        <IAO_0000117 rdf:datatype="&xsd;string">Person:Alan Ruttenberg</IAO_0000117>
        <IAO_0000119 rdf:datatype="&xsd;string">Person:Alan Ruttenberg</IAO_0000119>
        <IAO_0000119 rdf:datatype="&xsd;string">Person:Helen Parkinson</IAO_0000119>
        <IAO_0000117 rdf:datatype="&xsd;string">Person:Helen Parkinson</IAO_0000117>
        <IAO_0000115 rdf:datatype="&xsd;string">Relating a legal person to an organization in the case where the legal person has a role as member of the organization</IAO_0000115>
        <IAO_0000111 rdf:datatype="&xsd;string">is member of organization</IAO_0000111>
        <rdfs:range rdf:resource="&obo;OBI_0000245"/>
        <owl:inverseOf rdf:resource="&obo;OBI_0001688"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&obo;NCBITaxon_9606"/>
                    <rdf:Description rdf:about="&obo;OBI_0000245"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


